{
  "name": "rubyforgood.org",
  "description": "Contributors are welcome to take on open issues that are marked \"help wanted\"! Please comment on the issue if you need more clarity/information or DM @meg-gutshall on the Ruby for Good Slack.",
  "author": "",
  "license": "ISC",
  "main": "index.js",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rubyforgood/rubyforgood.org.git"
  },
  "bugs": {
    "url": "https://github.com/rubyforgood/rubyforgood.org/issues"
  },
  "homepage": "https://github.com/rubyforgood/rubyforgood.org#readme",
  "scripts": {
    "test": "bundle exec jekyll serve --livereload",
    "blog-preview": "bundle exec jekyll serve --drafts --future --livereload",
    "build": "bundle exec jekyll build",
    "doctor": "bundle exec jekyll doctor",
    "lint:js": "eslint \"**/*.js\" --cache --fix",
    "lint:style": "stylelint \"**/*.{css,less,sass,scss}\" --fix",
    "lint:text": "stylelint \"**/*.{html,md,markdown}\" --fix",
    "stylelint-check": "stylelint-config-prettier-check",
    "prepare": "husky install"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-prettier"
    ],
    "rules": {
      "no-descending-specificity": null,
      "declaration-block-single-line-max-declarations": null
    },
    "ignoreFiles": [
      "assets/**/*.min.css",
      "assets/**/*.min.js",
      "*.js",
      "*.json",
      "_site",
      "_site/**",
      ".sass-cache",
      ".jekyll-cache",
      ".jekyll-metadata",
      "_config.yml",
      "_layouts/default.html",
      "node_modules/**"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "es2017": true,
      "jquery": true
    },
    "plugins": [
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "rules": {
      "curly": [
        "error",
        "all"
      ],
      "no-tabs": [
        "error",
        {
          "allowIndentationTabs": true
        }
      ],
      "no-console": "off",
      "semi": "warn",
      "prettier/prettier": [
        "error",
        {},
        {
          "usePrettierrc": false
        }
      ],
      "arrow-body-style": "off",
      "prefer-arrow-callback": "off"
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "allowImportExportEverywhere": true,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "ignorePatterns": [
      "assets/**/*.min.css",
      "assets/**/*.min.js",
      "*.css",
      "*.json",
      "_site",
      "_site/**",
      ".sass-cache",
      ".jekyll-cache",
      ".jekyll-metadata",
      "_config.yml",
      "_layouts/default.html",
      "node_modules/**"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "htmlWhitespaceSensitivity": "ignore",
    "overrides": [
      {
        "files": "**/*.js",
        "excludeFiles": "**/*.min.js",
        "options": {
          "singleQuote": true,
          "trailingComma": "none",
          "semi": false
        }
      }
    ]
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{css,less,sass,scss,html,md,markdown}": "stylelint --fix"
  },
